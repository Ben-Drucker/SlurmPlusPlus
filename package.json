{
	"name": "SLURMPlusPlus",
	"icon": "./SlurmPlusPlusIcon.png",
	"description": "SLURM++ [Public Version] - A GUI for HPC Resources inside VS Code",
	"version": "0.1.3",
	"publisher": "slurmtools",
	"private": false,
	"engines": {
		"vscode": "^1.103.0"
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"type": "webview",
					"id": "slurm.main",
					"name": "SLURM++"
				}
			]
		},
		"commands": []
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/jsdom": "^21.1.7",
		"@types/node": "^24.3.0",
		"@types/ssh2": "^1.15.5",
		"@types/vscode": "^1.103.0",
		"@typescript-eslint/eslint-plugin": "^8.40.0",
		"@typescript-eslint/parser": "^8.40.0",
		"@vscode/test-cli": "^0.0.11",
		"@vscode/test-electron": "^2.5.2",
		"eslint": "^9.34.0",
		"typescript": "^5.9.2",
		"vscode-test": "^1.6.1"
	},
	"dependencies": {
		"browserify": "^17.0.1",
		"jsdom": "^26.1.0",
		"node": "24.6",
		"node-ssh": "^13.2.1",
		"requirejs": "^2.3.7",
		"ssh2": "^1.17.0",
		"undici-types": "^7.15.0"
	},
	"compilerOptions": {
		"noImplicitAny": false
	},
	"type": "commonjs"
}